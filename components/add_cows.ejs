
<!--Add Cow Modal -->
<div class="modal fade" id="addCowModal" tabindex="-1" aria-labelledby="addCowModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <form id="addCowForm" method="post" action="/add" enctype="multipart/form-data">
        <div class="modal-header text-center">
          <h5 class="modal-title w-100" id="addCowModalLabel">Add Cow</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <div class="mb-3">
              <label for="formFile" class="form-label">Default file input example</label>
              <input class="form-control" type="file" id="formFile">
          </div>
          <div class="mb-3">
            <label for="tag" class="form-label">Tag</label>
            <input type="number" class="form-control" id="cowTag" name="tag"  placeholder="12456">
          </div>
          <div class="mb-3">
            <label for="weight" class="form-label">Weight</label>
            <input type="number" step=".001" class="form-control" id="cowWeight" name="tag">
          </div>
          <div class="mb-3">
            <label for="bcount" class="form-label">Breeding Count</label>
            <input type="number" class="form-control" id="cowBreedCount" name="tag">
          </div>
          <div class="mb-3">
            <label for="ltbreed" class="form-label">Last Time Breed</label>
            <input type="date" class="form-control" id="cowLAstTimeBreed" name="tag">
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-success">Add Cow</button>
        </div>
      </form>
    </div>
  </div>
</div>

